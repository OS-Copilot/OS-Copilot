          "url": {
            "type": "string",
            "title": "Url"
          }
        },
        "type": "object",
        "required": [
          "url"
        ],
        "title": "CaptionQueryItem"
      },
      "QueryItemV2": {
        "properties": {
          "query": {
            "type": "string",
            "title": "Query"
          },
          "top_k": {
            "anyOf": [
              {
                "type": "integer"
              },
