          }
        },
        "type": "object",
        "required": [
          "query"
        ],
        "title": "QueryItemV2"
      },
      "TargetPageModel": {
        "properties": {
          "url": {
            "type": "string",
            "title": "Url"
          }
        },
        "type": "object",
        "required": [
          "url"
        ],
        "title": "TargetPageModel"
      },      
      "SQLRequest": {
        "properties": {
          "queries": {
            "items": {
              "type": "string"
            },
